{
  "date1": {
    "prefix": ["rfc3339"],
    "body": [
      "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}T${CURRENT_HOUR}:${CURRENT_MINUTE}:${CURRENT_SECOND}"
    ],
    "description": "rfc3339"
  },
  "date2": {
    "prefix": ["dt"],
    "body": [
      "${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}"
    ],
    "description": "date only"
  },
  "date3": {
    "prefix": ["date"],
    "body": [
      "${CURRENT_DAY_NAME_SHORT} ${CURRENT_DATE}-${CURRENT_MONTH_NAME_SHORT}-${CURRENT_YEAR}"
    ],
    "description": "date (with day)"
  },
  "date4": {
    "prefix": ["date"],
    "body": [
      "${CURRENT_DATE}-${CURRENT_MONTH_NAME_SHORT}-${CURRENT_YEAR}"
    ],
    "description": "date (with day)"
  }
}
